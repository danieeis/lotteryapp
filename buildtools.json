{
    "$schema": "https://mobilebuildtools.com/schemas/v2/buildtools.schema.json",
    "appConfig": {
        "strategy": "TransformOnly",
        "disable": false
    },
    "artifactCopy": {
        "disable": false
    },
    "automaticVersioning": {
        "behavior": "PreferBuildNumber",
        "environment": "All",
        "versionOffset": 0,
        "disable": false
    },
    "css": {
        "minify": false,
        "bundleScss": false,
        "disable": false
    },
    "images": {
        "directories": [
            "Images"
        ],
        "conditionalDirectories": {
            "ios": [
                "Images//iOS"
            ],
            "android": [
                "Images//Android"
            ]
        },
        "watermarkOpacity": null,
        "disable": false
    },
    "manifests": {
        "token": "$",
        "variablePrefix": "Manifest_",
        "missingTokensAsErrors": true,
        "disable": false
    },
    "releaseNotes": {
        "maxDays": 7,
        "maxCommit": 10,
        "characterLimit": 250,
        "filename": "ReleaseNotes.txt",
        "createInRoot": false,
        "disable": false
    },
    "projectSecrets": {
        "FloridaLottery": {
            "className": "Settings",
            "namespace": "Helpers",
            "properties": [
                {
                    "name": "AppCenterIdDroid",
                    "type": "String"
                }
            ]
        }
    },
    "environment": {
        "defaults": {
            "AppId": "com.jrestrepo.florida_lottery"
        },
        "configuration": {
            "Debug": {
                "AppId": "com.jrestrepo.florida_lotterydev"
            }
        }
    },
    "debug": true
}